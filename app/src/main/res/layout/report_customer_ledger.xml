<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_doc_date"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/thirteen_sp" />

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_doc_no"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/thirteen_sp" />

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_ref_org_un"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/text_size_small" />

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_dr_amt"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/text_size_small" />

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_cr_amt"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/thirteen_sp" />

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_channel_txt"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/thirteen_sp" />

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_div_txt"

            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/text_size_small" />

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_total_cr"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/text_size_small" />

        <com.luminous.mpartner.custom_text_view.TextViewRegular
            android:id="@+id/tv_cdrc"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:textSize="@dimen/text_size_small" />

    </LinearLayout>

</layout>